<meta http-equiv="Content-Type" content="text/html;charset=utf-8">
<base target="iframe"/>
<head>
	<title>欢迎页面</title>
</head>
<body>
	<h1>欢迎使用李晨军的网站后台系统</h1>
	<div class="back">
	<table class="table">
		<tr>
			
			<th>
				服务器配置
			</th>
			<th>
				参数
			</th>
			
		</tr>
		<volist name="data.sys" id="v">
			<tr>
				
				<td>
					{$v.0}
				</td>
				<td>
					{$v.1}

				</td>
			</tr>
		</volist>
		
	</table>
	</div>
	<div class="back">
	<table class="table">
		<tr>
			
			<th>
				个人配置
			</th>
			<th>
				参数
			</th>
			
		</tr>
		<volist name="data.user" id="v">
			<tr>
				
				<td>
					{$v.0}
				</td>
				<td>
					{$v.1}

				</td>
			</tr>
		</volist>
		
	</table>
</div>
