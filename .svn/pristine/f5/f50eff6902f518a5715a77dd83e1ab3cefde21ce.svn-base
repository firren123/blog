/*顶部样式*/
#top{width:100%;height:26px;background: #157AD4;border-bottom: 2px solid #156DCF;}
#top .top-m{width: 1190px;margin: 0 auto;
	font-family:  Lucida Grande,tahoma,arial,宋体;color: #C2D9F8; }
#top .top-m a{text-decoration: none;color: #C2D9F8;line-height: 26px;}
#top .top-m i.top-l{border-right:1px solid #156DCF; height: 12px;
	margin: 8px 12px 0;color:#157AD4;overflow: hidden;}
#top .top-m .top-left{display: block;float: left;}
#top .top-m .top-left #shoujipng{background:url("../Images/hdft.png") no-repeat -300px 0px;
width: 11px;height:17px;float: left;margin: 8px 3px 0}
#top .top-m .top-right{display: block;float: right;}
/*顶部样式结束*/
/*搜索部分*/
#nav-bg{width: 100%;height: 183px;background: #1D7AD9;}
#nav-bg .nav-m{
	width: 1190px;margin: 0 auto;position: relative;
}
#nav-bg .nav-m .close{
	display: block;
	width:15px;
	height: 15px;
	background-color: #ffffe3;
	background-image:url("../Images/hint.png");
	background-position: -18px 3px;
	cursor: pointer;
	position: absolute;
	z-index: 999;
	top:5px;
	right: 10px;
	overflow: hidden;
}
#nav-bg .nav-m .adimg{height: 80px;}
