/*导航部分*/
#nav2-bg{width:100%;height: 40px;background: #1369C0;}
#nav2-bg .nav-m{width: 1190px;margin: 0 auto;}
#nav2-bg .nav-m #cate{width: 190px;float: left;height: 39px;
